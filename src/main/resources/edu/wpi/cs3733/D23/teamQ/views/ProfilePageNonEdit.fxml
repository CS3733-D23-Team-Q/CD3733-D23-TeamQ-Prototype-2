            <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="150.0">
               <children>
                  <HBox fx:id="Title_Display" alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="Title_Display" text="Label" HBox.hgrow="ALWAYS" />
                     </children>
                  </HBox>
                  <HBox fx:id="ID_Number_Display" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label fx:id="ID_Number_Display" text="Label" />
                     </children>
                  </HBox>
                  <HBox fx:id="First_Name_Display" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="Label" />
                     </children>
                  </HBox>
                  <HBox fx:id="Last_Name_Display" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="Label" />
                     </children>
                  </HBox>
                  <HBox fx:id="Username_Display" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="Label" />
                     </children>
                  </HBox>
                  <HBox fx:id="Email_Display" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="Label" />
                     </children>
                  </HBox>
                  <HBox fx:id="Phone_Number_Display" alignment="CENTER" prefHeight="100.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
                     <children>
                        <Label text="Label" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </HBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="50.0" prefWidth="518.0" BorderPane.alignment="CENTER">
         <children>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Button fx:id="Edit_Profile" alignment="CENTER" mnemonicParsing="false" text="Edit Profile" />
               </children>
            </VBox>
         </children>
      </HBox>
   </bottom>
   <top>
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
